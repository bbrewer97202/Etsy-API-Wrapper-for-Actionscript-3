******************************************************************************/		public function EtsyAPIRequestEvent(type:String, status:int, resultsValue:String, message:String, bubbles:Boolean = false, cancelable:Boolean = false) {						super(type, bubbles, cancelable);			httpStatus = status;			results = resultsValue;			text = message;		}		/***************************************************************************************		 * override clone 		 ***************************************************************************************/           		public override function clone():Event {			return new EtsyAPIRequestEvent(type, httpStatus, results, text, cancelable);		}	     		/***************************************************************************************		 * override toString 		 ***************************************************************************************/		public override function toString():String {			return formatToString("EtsyAPIRequestEvent", "type", "httpStatus", "results", "text", "bubbles", "cancelable", "eventPhase");		}			}}